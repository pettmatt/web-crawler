{
	"name": "scraper-link-validator",
	"main": "./index.js",
	"version": "0.0.1",
	"type": "module",
	"description": "Validate if a page allows bot activity",
	"jest": {
		"transform": {
			"^.+\\.jsx?$": "babel-jest"
		},
		"testEnvironment": "node",
		"moduleFileExtensions": [
			"js",
			"mjs"
		],
		"testMatch": [
			"**/*.test.js",
			"**/*.test.mjs"
		]
	},
	"scripts": {
		"start": "concurrently --kill-others \"node ./receiver\"",
		"dev": "concurrently --kill-others \"nodemon ./receiver\"",
		"test": "jest --detectOpenHandles",
		"lint": "eslint ."
	},
	"keywords": [
		"validator",
		"web-scraper"
	],
	"devDependencies": {
		"@babel/core": "^7.23.2",
		"@babel/preset-env": "^7.23.2",
		"@types/jest": "^29.5.6",
		"babel-jest": "^29.7.0",
		"concurrently": "^8.2.2",
		"jest": "^29.7.0",
		"nodemon": "^3.0.1"
	},
	"dependencies": {
		"amqplib": "^0.10.3"
	}
}
